{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "providers": ["staticfile"]
    }
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "sleepApplication": false,
    "multiRegionConfig": {
      "asia-southeast1-eqsg3a": {
        "numReplicas": 1
      }
    },
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "static": {
    "publish_dir": "."
  }
}
